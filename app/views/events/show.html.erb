<h1> <%= @event.title %> </h1>
<ul>
    <li>Friend: <%= link_to @event.friend.full_name, friend_path(@event.friend) %></li>
    <li>Location: <%= @event.title %></li>
    <li>Event Time: <%= @event.readable_time %></li>

 </ul>

 <%= button_to "Edit Event", edit_user_event_path(@event.user, @event), method: "get" %> 
 <br>
 <%= button_to "Delete Event", user_event_path(@event.user, @event), method: "delete" %>