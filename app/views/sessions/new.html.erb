<h1> Login </h1>
<%= form_with model: @user, url:"/login", method: "post" do |f| %>

<%= f.label :email, "Email:" %>
<%= f.text_field :email %> <br> <br>

<%= f.label :password, "Password:" %>
<%= f.password_field :password %> <br> <br>

<%= submit_tag "Login"%>
<% end %>
<br>
<a href="/auth/google_oauth2" class="btn btn-primary">Login with Google</a>
